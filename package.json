{
  "name": "korean-spell-checker-vs-code",
  "displayName": "Korean Spell Checker VS Code",
  "description": "",
  "version": "0.2.4",
  "engines": {
    "vscode": "^1.38.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hannut91/korean-spell-checker-vs-code.git"
  },
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:spell.check",
    "onCommand:spell.fix"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spell.check",
        "title": "Korean Spell Check"
      },
      {
        "command": "spell.fix",
        "title": "Korean Spell Fix"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "./node_modules/.bin/tslint -p ."
  },
  "devDependencies": {
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.1",
    "@types/vscode": "^1.38.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.21"
  },
  "publisher": "Yunseok"
}
