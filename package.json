{
	"name": "korean-spell-checker-vs-code",
	"displayName": "Korean Spell Checker VS Code",
	"description": "",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hannut91/korean-spell-checker-vs-code.git"
	},
	"activationEvents": [
		"onCommand:extension.spell"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.spell",
				"title": "Korean Spell Checker: spell"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"axios": "^0.19.0"
	},
	"publisher": "Yunseok"
}
